{
  "name": "multiset-permutation",
  "version": "0.0.3",
  "description": "Iterative generation of all permutations of a multiset",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": [
    "permutation",
    "multiset",
    "coollex",
    "Williams"
  ],
  "author": "Robert",
  "license": "ISC",
  "bugs": {
    "url": "http://github.com/rbrincke/multiset-permutation/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/rbrincke/multiset-permutation.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "dist"
  ]
}
