{
  "name": "multiset-permutation",
  "version": "0.0.2",
  "description": "Iterative generation of all permutations of a multiset",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": [
    "permutation",
    "multiset",
    "coollex",
    "Williams"
  ],
  "author": "Robert",
  "license": "ISC",
  "bugs": {
    "url": "http://github.com/roberttenbrincke/multiset-permutation/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/roberttenbrincke/multiset-permutation.git"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "jest": "^22.4.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "dist"
  ]
}
